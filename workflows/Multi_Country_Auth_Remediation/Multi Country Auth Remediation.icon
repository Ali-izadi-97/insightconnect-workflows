          "08a83567-15b7-4fc3-86a8-8503d3d740b9": {
            "nodeId": "08a83567-15b7-4fc3-86a8-8503d3d740b9",
            "name": "Subtract VPN Count",
            "type": "action",
            "plugin": {
              "name": "Python 3 Script",
              "slugVendor": "rapid7",
              "slugName": "python_3_script",
              "slugVersion": "2.0.0",
              "imageData": "https://us.cdn-assets.connect.insight.rapid7.com/icons/rapid7/python_3_script/2.0.0/icon.png"
            },
            "identifier": "run",
            "continueOnFailure": false,
            "isDisabled": false,
            "parameters": {
              "input": {
                "function": "def run(params={}):\n    vpn_ips = params.get(\"vpn_ips\")\n    alert_ips = params.get(\"alert_ips\")\n    if not alert_ips:\n        return {'count': -1}\n    \n    minuend = len(alert_ips)\n    for alert_ip in alert_ips:\n        if alert_ip['ip'] in vpn_ips:\n            minuend -= 1\n            \n    return {'count': minuend}",
                "input": {
                  "alert_ips": [
                    {
                      "ip": "127.0.0.1"
                    },
                    {
                      "ip": "129.2.4.1"
                    },
                    {
                      "ip": "130.2.4.1"
                    }
                  ],
                  "vpn_ips": [
                    "127.0.0.1",
                    "9.9.9.9"
                  ]
                }
              }
            },
